        "build": "node scripts/build.js && node server/app.js",
        "build:serve": "serve build",
        "report": "npm run build --withReport true",
        "start:server": "node server/app.js",
        "start:server-dev": "nodemon node server/app.js",
        "test": "node scripts/test.js",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "test:update": "jest -u",
        "storybook": "start-storybook -p 9009 -s public",
