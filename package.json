        "lint": "eslint \"**/*.{ts,tsx}\" --fix",
        "lint:format": "npm run lint && npm run format",
        "release": "standard-version"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "npm run format",
            "npm run lint",
